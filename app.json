{
  "expo": {
    "name": "Cetur Passenger",
    "slug": "PassengerMobil",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let user qr code scaning.",
        "NSLocationWhenInUseUsageDescription": "In order to manage the covid19 filation process, the location data are collected by using the 'I got off the vehicle-got on the vehicle' function in order to track who got on the vehicle from which locations."
      },
      "bundleIdentifier": "com.ceturpassenger.cetur",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.ceturpassenger.cetur",
      "versionCode": 1,
      "permissions": ["CAMERA", "ACCESS_FINE_LOCATION" ],
      "icon": "./assets/icon.png"
    },
    "description": ""
  }
}
